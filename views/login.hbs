            <input type="button" value="Login" onclick="myFunction()">
            {{!-- <a href="#">Lost your password?</a><br> --}}
            <a href="signUp">Don't have an account?</a>
        </form>
    </div>
    <script type="text/javascript" src="js/loginScript.js"></script>    
